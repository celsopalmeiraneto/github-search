{
    "env": {
        "browser": false,
        "es6": true,
        "node": true,
        "jest/globals": true
    },
    "extends": ["eslint:recommended", "plugin:jest/recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 8
    },
    "plugins": ["jest"],
    "rules": {
      "max-len": ["error",
        {"ignorePattern": "^\\s*const\\s.+=\\s*require\\s*\\(" },
        {"ignoreComments": true}
      ],
      "no-console": "off"
    }
}
